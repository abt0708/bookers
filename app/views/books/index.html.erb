<h1>Books</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><span><%= book.title %></span></td>
      <td><span><%= book.body %></span></td>
      <td><%= link_to "show", "/books/#{book.id}" %></td>
      <td><%= link_to "Edit", "/books/edit" %></td>
      <td><%= link_to "Destroy", "/books/destroy" %></td>
    </tr>
  <% end %>
</table>
